{
  "routes": [
    { "src": "/api/summoners/(<id>[^/]*)", "methods": ["DELETE", "PUT"], "dest": "/api/summoners/$id"},
    { "src": "/api/summoners", "methods": ["POST", "GET", "DELETE", "PUT"], "dest": "/api/summoners"},
    {
        "src": "/images/(.*)",
        "headers": { "cache-control": "s-maxage=604800" },
        "dest": "/images/$1"
    }
  ]
}