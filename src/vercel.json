{
  "routes": [
    {
      "src": "/images/spells/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "/images/spells/$1"
    },
    {
      "src": "/images/champion/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "/images/champion/$1"
    },
    {
      "src": "/images/item/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "/images/item/$1"
    },
    {
      "src": "/images/runes/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "/images/runes/$1"
    },
    {
      "src": "/images/spell/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "/images/spell/$1"
    },
    {
      "src": "/images/shards/(.*)",
      "headers": { "cache-control": "max-age=31536000, immutable" },
      "dest": "/images/shards/$1"
    },
    { "handle": "filesystem" },
    { "src": "/api/summoners/(.*)", "methods": ["POST", "DELETE", "PUT"], "dest": "/api/summoners/$1"},
    { "src": ".*", "dest": "/" , "methods": ["POST", "GET", "DELETE", "PUT"]},
    { "src": "/api/summoners", "methods": ["POST", "GET", "DELETE", "PUT"], "dest": "/api/summoners"},
    { "src": "/api/builds", "methods": ["POST", "GET", "DELETE", "PUT"], "dest": "/api/builds"}
  ]
}